@model TCS.WebUI.Models.LoginViewModel
@{
    ViewData["PageName"] = "login";
    Layout = null;
}

<!DOCTYPE html>
<partial name="_CopyrightHeader" />
<html lang="es-CL">
<head>
    <partial name="_Head" />
    <link rel="stylesheet" media="screen, print" href="~/css/fa-brands.css">
    <link rel="stylesheet" href="~/css/notifications/sweetalert2/sweetalert2.min.css">
</head>
<body>
    <partial name="_CookieConsentPartial" />
    <!-- BEGIN Page Wrapper -->
    <div class="page-wrapper">
        <div class="page-inner bg-brand-gradient" style="background-image: url('img/card-backgrounds/telas-azules fullhd.jpg'); background-size: cover; ">
            <div class="page-content-wrapper bg-transparent">
                @*<partial name="_PageHeader" />*@
                <div class="container-fluid col-lg-3 col-md-5 col-sm-6 col-7">
                    <div class="row d-flex justify-content-center">
                        <div>
                            <h2 class="fs-xxl fw-500 text-white">
                                Bienvenidos a TCS
                            </h2>
                            <h1 class="text-white fw-300 mb-3 ">
                                Login de TCS
                            </h1>
                            <div class="card p-4 rounded-plus bg-faded">
                                <form asp-route="auth" method="post">
                                    <div asp-validation-summary="All" class="alert alert-primary text-dark"></div>
                                    @*<div class="form-group">
                                <label class="form-label">@SharedLocalizer["domain"] :</label>
                                <select asp-for="Domain" class="form-control" asp-items="ViewBag.Domain"></select>
                            </div>*@
                                    <div class="form-group">
                                        <label class="form-label">Cuenta</label>
                                        <input asp-for="Username" type="text" class="form-control" placeholder="Usuario de windows" />
                                        <span asp-validation-for="Username" class="invalid-feedback">Faltó la cuenta</span>
                                        <div class="help-block">Si no recuerdas tu cuenta de windows comunicate con el administrador.</div>
                                    </div>
                                    <div class="form-group">
                                        <label class="form-label">Contraseña</label>
                                        <input type="password" asp-for="Password" class="form-control" placeholder="Contraseña" />
                                        <span class="invalid-feedback" asp-validation-for="Password">Hey!, sin contraseña no hay paraiso.</span>
                                        <div class="help-block">En caso de no recordar tu contraseña comunicate con el administrador.</div>
                                    </div>
                                    <div class="row no-gutters">
                                        <div class="col-lg-12 pl-lg-1 my-2">
                                            <button id="js-login-btn" type="submit" class="btn btn-primary btn-block btn-lg">Iniciar sesión</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container">
