<script>
    const buttonLogin = document.querySelector('#js-login-btn');
    buttonLogin.addEventListener('click', () =>
    {
        buttonLogin.disabled = true;
        buttonLogin.value = 'Submitting...';
        buttonLogin.form.submit();
    });

    const divDownloads = document.querySelector('#div-downloads');
    const templateAndroid = document.querySelector('#template-android');
    const templateIos = document.querySelector('#template-ios');
    const templatePcAndroid = document.querySelector('#template-pc-android');
    const templatePcIos = document.querySelector('#template-pc-ios');

    getOS();
    function getOS() 
    {
        let userAgent = window.navigator.userAgent,
        platform = window.navigator?.userAgentData?.platform ?? window.navigator.platform,
        macosPlatforms = ['Macintosh', 'MacIntel', 'MacPPC', 'Mac68K'],
        windowsPlatforms = ['Win32', 'Win64', 'Windows', 'WinCE'],
        iosPlatforms = ['iPhone', 'iPad', 'iPod'],
        os = null;

      if (macosPlatforms.indexOf(platform) !== -1) 
      {
        os = 'Mac OS';

        const clon = templatePcAndroid.content.cloneNode(true);
        divDownloads.appendChild(clon);
        const clon2 = templatePcIos.content.cloneNode(true);
        divDownloads.appendChild(clon2);
        
        const buttonAndroidIos = document.querySelector('#button-pc-android');
        buttonAndroidIos.addEventListener('click', ShowAndroidQr);

        const buttonPcIos = document.querySelector('#button-pc-ios');
        buttonPcIos.addEventListener('click', ShowIosQr);
      } 
      else if (iosPlatforms.indexOf(platform) !== -1) 
      {
        os = 'iOS';
        const clon = templateIos.content.cloneNode(true);
        divDownloads.appendChild(clon);
      } 
      else if (windowsPlatforms.indexOf(platform) !== -1) 
      {
        os = 'Windows';
        //const clon = templateAndroid.content.cloneNode(true);
        //divDownloads.appendChild(clon);
        //const clon = templateIos.content.cloneNode(true);
        //divDownloads.appendChild(clon);

        const clon = templatePcAndroid.content.cloneNode(true);
        divDownloads.appendChild(clon);
        const clon2 = templatePcIos.content.cloneNode(true);
        divDownloads.appendChild(clon2);
        
        const buttonAndroidIos = document.querySelector('#button-pc-android');
        buttonAndroidIos.addEventListener('click', ShowAndroidQr);

        const buttonPcIos = document.querySelector('#button-pc-ios');
        buttonPcIos.addEventListener('click', ShowIosQr);
      } 
      else if (/Android/.test(userAgent)) 
      {
        os = 'Android';
        const clon = templateAndroid.content.cloneNode(true);
        divDownloads.appendChild(clon);
      } 
      else if (!os && /Linux/.test(platform)) 
      {
        os = 'Linux';
        const clon = templatePcAndroid.content.cloneNode(true);
        divDownloads.appendChild(clon);
        const clon2 = templatePcIos.content.cloneNode(true);
        divDownloads.appendChild(clon2);
      }

      //if (os !="Windows"){

      //  Swal.fire({
      //                      icon: 'error',
      //          text:"Runs only on Windows",
      //          showConfirmButton: false,
      //          allowOutsideClick: false,
      //          allowEscapeKey: false
      //      })
       
    
     
      //}
    }

    function ShowAndroidQr()
    {
        Swal.fire({
            imageUrl: `img/qr/qrandroid.png`,
            imageWidth: 400,
            imageHeight: 400,
            showCancelButton: false,
            showConfirmButton: false,
        });
    }

    function ShowIosQr()
    {
        Swal.fire({
            imageUrl: `img/qr/qrios.png`,
            imageWidth: 400,
            imageHeight: 400,
            showCancelButton: false,
            showConfirmButton: false,
        });
    }
</script>